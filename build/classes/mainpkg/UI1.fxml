      <Button fx:id="loadPieChartButton" layoutX="371.0" layoutY="403.0" mnemonicParsing="false" onAction="#loadPieChartButtonOnClick" text="Load Pie Chart" />
      <PieChart fx:id="pieChart" layoutX="60.0" layoutY="24.0" prefHeight="304.0" prefWidth="466.0" style="-fx-border-color: black;" title="Pie Chart" />
      <Label fx:id="statusLabel" layoutX="60.0" layoutY="342.0" onMouseEntered="#labelMouseEnterOnClick" prefHeight="35.0" prefWidth="347.0" style="-fx-border-color: black;" textFill="#0c1cf5">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="goBack" layoutX="500.0" layoutY="403.0" mnemonicParsing="false" onAction="#goBackOnClick" text="Go Back" />
   </children>
</AnchorPane>
